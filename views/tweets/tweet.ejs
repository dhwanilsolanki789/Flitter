<% layout('layouts/boilerplate.ejs') -%>
    <h1>Home | Flitter</h1>
    <hr>

    <div class="offset-2 col-8">
        <div class="container py-3">
            <a href="/fleets/new" style="margin-left: 20px;">New Fleet</a>
        </div>
        <ul>
            <% for(let t of allTweets) { %>
                <li>
                    <strong>
                        <%= t.username %>
                    </strong> </br>
                    <%= t.tweet %> <a href="/fleets/<%= t._id%>">Details</a>
                </li> </br>
                <% } %>
        </ul>
    </div>